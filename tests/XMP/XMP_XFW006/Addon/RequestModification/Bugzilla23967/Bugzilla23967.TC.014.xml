		<selector urlPattern="http://www.bad.com/index/test1.html">
			<modifyHeader name="REQMOD_MatchedPattern" value="REQMOD_MatchedPattern"/>
			<modifyHeader name="REQMOD_MatchedData" value="REQMOD_MatchedData + 'abc'"/>
