<test>
       
      <description> SOAP-API-Settings{(setCommunityFilter( )}: Testing the setting of a community filter for a Community with scheme set as anyother value other than 0,1,2. </description>
     
	<function name="testName" module="Basic">
		<parameter>XMP-Settings-4.4.8.0</parameter>
	</function>
	
	<function name="SOAPClient" module="SOAP_API" >
		<parameter>
			<configuration>XMP/PROTOCOL</configuration>
			<configuration>XMP/IP</configuration>
			<parameter>:</parameter>
			<configuration>XMP/PORT/</configuration>
			<parameter>/jaxws-policy-communities/communities</parameter>
		</parameter>
		<file>addcommunity4.xml</file>
		<parameter>Body/return</parameter>
	</function>
	<function name="checkResponseCode" module="SOAP_API">
		 <parameter>200</parameter>
	</function>
	  
	<function name="SOAPClient" module="SOAP_API" >
		<parameter>
			<configuration>XMP/PROTOCOL</configuration>
			<configuration>XMP/IP</configuration>
			<parameter>:</parameter>
			<configuration>XMP/PORT/</configuration>
			<parameter>/jaxws-policy-settings/settings</parameter>
		</parameter>
		<file>setCommunityFilter-scheme-set-to-3.xml</file>
		<parameter>Body/return</parameter>
	</function>
	<function name="checkResponseCode" module="SOAP_API">
		 <parameter>500</parameter>
	</function>

	<function name="SOAPClient" module="SOAP_API" >
		<parameter>
			<configuration>XMP/PROTOCOL</configuration>
			<configuration>XMP/IP</configuration>
			<parameter>:</parameter>
			<configuration>XMP/PORT/</configuration>
			<parameter>/jaxws-policy-communities/communities</parameter>
		</parameter>
		<file>destroycommunity4.xml</file>
		<parameter>Body/return</parameter>
	</function>
	<function name="checkResponseCode" module="SOAP_API">
		 <parameter>200</parameter>
	</function>
        
 </test>
