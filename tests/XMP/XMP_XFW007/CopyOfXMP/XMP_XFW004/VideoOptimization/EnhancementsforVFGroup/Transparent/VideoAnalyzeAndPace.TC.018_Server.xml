	</function>

	<!-- Sleep a while until video optimization will start -->
	<function name="sleep" module="Basic">
		<parameter>75000</parameter>
	</function>

	<!-- Stop Web Server -->
	<function name="stopWebServer" module="HTTP"/>

</test>
